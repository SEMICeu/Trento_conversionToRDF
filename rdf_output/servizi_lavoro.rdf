  <rdf:Description rdf:about="http://www.provincia.tn.it/p_TN_NP61">
    <dct:title xml:lang="it">Convenzioni di programma per l'inserimento lavorativo di persone con disabilità</dct:title>
    <cv:thematicArea rdf:resource="http://publications.europa.eu/resource/authority/data-theme/ECON"/>
