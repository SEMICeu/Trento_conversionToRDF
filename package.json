{
  "name": "learn-nightwatch",
  "version": "1.0.24",
  "description": "Learn how to use nightwatch for front-end testing. step-by-step tutorial",
  "main": "index.js",
  "engines": {
    "node": "4.4.6"
  },
  "scripts": {
    "test": "run-s clean convert nightwatch",
    "ie": "nightwatch -e ie11",
    "sauce": "nightwatch -e chrome,ie11,android_s4_emulator,iphone_6_simulator",
    "upload": "node test/e2e/upload_screenshots_to_s3.js",
    "e2e": "npm run sauce; npm run upload",
    "dev": "nodemon -w test/index.html --delay 250ms --exec 'npm run upload'",
    "nightwatch": "nightwatch --env local",
    "clean:reports": "rimraf reports/*",
    "clean:screenshots": "rimraf screenshots/*",
    "clean:downloads": "rimraf downloads/*",
    "clean:tmp": "rimraf tmp/*",
    "clean": "run-p clean:reports clean:screenshots clean:downloads clean:tmp",
    "convert": "run-p convertSpain convertPortugal convertTrento",
    "convertSpain": "node exceljson_Spain.js",
    "convertPortugal": "node exceljson_Portugal.js",
    "convertTrento": "node exceljson_Trento.js",
    "cleanconvert": "run-s clean convert"
},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwyl/learn-nightwatch.git"
  },
  "author": "DWYL & Friends",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/dwyl/learn-nightwatch/issues"
  },
  "homepage": "https://github.com/dwyl/learn-nightwatch#readme",
  "dependencies": {
    "convert-excel-to-json": "^1.4.0",
    "glob": "^7.1.3",
    "nightwatch": "^0.9.21",
    "npm-run-all": "4.1.2",
    "rimraf": "^2.6.2",
    "saucelabs": "^1.3.0",
    "selenium-download": "^2.0.7"
  },
  "devDependencies": {
    "aws-sdk": "^2.7.13",
    "env2": "^2.2.0",
    "geckodriver": "^1.16.0",
    "mime-types": "^2.1.13",
    "nodemon": "^1.11.0",
    "selenium-server": "3.4.0"
  },
  "keywords": [
    "nightwatch",
    "testing",
    "front-end",
    "UI",
    "browser",
    "test",
    "testing",
    "step-by-step tutorial",
    "how to",
    "User Acceptance Testing",
    "UAT"
  ]
}
